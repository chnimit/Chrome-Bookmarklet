javascript:(function() {  function extractLinksFromSitemap() {    const locElements = document.getElementsByTagName('loc');    const links = Array.from(locElements).map(el => el.textContent.trim());    let htmlContent = '<html><head><title>Sitemap Links</title>';    htmlContent += '<style>ul { list-style-type: none; } li { margin-bottom: 5px; }</style>';    htmlContent += '</head><body>';    htmlContent += `<h1>Sitemap Links</h1>`;    htmlContent += `<p>Total Links: ${links.length}</p>`;    htmlContent += '<button id="downloadCsv">Download as CSV</button> ';    htmlContent += '<button id="copyUrls">Copy URLs</button>';    htmlContent += '<ul>';    links.forEach((link, index) => {      htmlContent += `<li>${index + 1}. <a href="${link}" target="_blank">${link}</a></li>`;    });    htmlContent += '</ul>';    htmlContent += '</body></html>';    const newWindow = window.open('', '_blank');    newWindow.document.write(htmlContent);    newWindow.document.close();    newWindow.document.getElementById('downloadCsv').addEventListener('click', function() {      let csvContent = 'URL\n';      links.forEach(link => {        csvContent += link + '\n';      });      const blob = new Blob([csvContent], { type: 'text/csv;charset=utf-8;' });      const url = URL.createObjectURL(blob);      const a = newWindow.document.createElement('a');      a.href = url;      a.download = 'sitemap_links.csv';      newWindow.document.body.appendChild(a);      a.click();      newWindow.document.body.removeChild(a);      URL.revokeObjectURL(url);    });    newWindow.document.getElementById('copyUrls').addEventListener('click', function() {      const textArea = newWindow.document.createElement('textarea');      textArea.value = links.join('\n');      newWindow.document.body.appendChild(textArea);      textArea.select();      newWindow.document.execCommand('copy');      newWindow.document.body.removeChild(textArea);      alert('URLs copied to clipboard!');    });  }  extractLinksFromSitemap();})();
