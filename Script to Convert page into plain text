javascript:(function(){let scripts=document.querySelectorAll('script');scripts.forEach(script=>script.remove());let styles=document.querySelectorAll('style, link[rel="stylesheet"]');styles.forEach(style=>style.remove());let elements=document.querySelectorAll('*[style]');elements.forEach(element=>element.removeAttribute('style'));let noscripts=document.querySelectorAll('noscript');noscripts.forEach(noscript=>noscript.remove());let allElements=document.querySelectorAll('*');allElements.forEach(el=>{el.outerHTML=el.outerHTML.replace(/ on\w+="[^"]*"/g,'');});console.clear();console.log(document.documentElement.outerHTML);})();
